{
  "name": "sprintos-api",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "repository": "https://github.com/QUANG221222/Sprintos-API.git",
  "scripts": {
    "lint": "eslint src --ext ts --report-unused-disable-directives --max-warnings 0",
    "clean": "rm -rf build && mkdir build",
    "clean-dist": "rm -rf dist && mkdir dist",
    "build-babel": "babel ./src -d ./dist --extensions \".ts\"",
    "build": "npm run clean-dist && npm run build-babel",
    "production": "npm run build && cross-env BUILD_MODE=production node ./dist/server.js",
    "dev": "cross-env BUILD_MODE=dev nodemon --exec babel-node --extensions \".ts\" ./src/server.ts",
    "seed": "cross-env BUILD_MODE=dev babel-node --extensions \".ts\" src/scripts/seedData.ts"
  },
  "keywords": [
    "sprintos",
    "api",
    "sprintos-api",
    "sprint",
    "tool",
    "task management"
  ],
  "author": "WangCoder2k5",
  "license": "MIT",
  "packageManager": "pnpm@10.17.0",
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.10",
    "@babel/node": "^7.28.0",
    "@babel/plugin-transform-runtime": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/js": "^9.39.2",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lodash": "^4.17.20",
    "@types/ms": "^2.1.0",
    "@types/multer": "^2",
    "@types/node": "^24.5.2",
    "@types/uuid": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "baseline-browser-mapping": "^2.9.11",
    "eslint": "^9.36.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.4",
    "@getbrevo/brevo": "^3.0.1",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.7.0",
    "connect-redis": "^5.2.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cross-env": "^10.0.0",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "http-status-codes": "^2.3.0",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mongodb": "6.9.0",
    "ms": "^2.1.3",
    "multer": "^2.0.2",
    "multer-storage-cloudinary": "^4.0.0",
    "socket.io": "^4.8.1",
    "uuid": "^9.0.1"
  }
}
